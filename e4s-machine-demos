#example deployment e4s-machine-demos VM
apiVersion: vmoperator.vmware.com/v1alpha1
kind: VirtualMachine
metadata:
  name: e4s-machine-demos
  namespace: default
  annotations:
    vmoperator.vmware.com/image-supported-check: disable
spec:
  className: my-vm-class-8
  imageName: ent.box.com/shared/static/k9g0irliylbtethanze5ajkp9yuhummh.ova 
  powerState: poweredOn
  networkInterfaces:
  - networkName: "VM Network"
    ethernetCardType: "vmxnet3"

---
apiVersion: vmoperator.vmware.com/v1alpha1
kind: VirtualMachineClass
metadata:
  name: my-vm-class-8
spec:
  hardware:
    cpus: 4
    memory: 6Gi
